---
import "../styles/global.css";

interface OldLayout {
    title: string;
    icon: string;
}
const { title } = Astro.props;
const { icon } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{ title }</title>
    <link rel="icon" href={ icon }>
</head>
<slot />
</html>